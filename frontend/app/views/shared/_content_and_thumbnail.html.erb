<% if thumbnail_available?(record) %>
  <div class="row">
    <div class="col-sm-9">
      <%= content %>
    </div>
    <div class="col-sm-3">
      <%= render_aspace_partial :partial => "digital_objects/thumbnail_for_file_version", :locals => {:file_version => fetch_thumbnail(record)} %>
    </div>
  </div>
<% else %>
  <%= content %>
<% end %>